<h4>修改密碼</h4>
<div class="row">
	<div class="col-md-10 offset-md-2">
		<div class="row">
			<div class="col-10">
				<div class="form-group">
					<label for="input-resetPassword">新密碼</label>
					<input type="password" class="form-control" id="input-resetPassword" placeholder="Password">
					<small class="form-text text-muted" id="password-warning">請輸入大於等於 <span class="text-danger">6</span> 個字元</small>
				</div>
			</div>
		</div>
		<div class="row">
			<div class="col-10">
				<div class="form-group">
					<label for="input-resetPasswordConfirm">確認新密碼</label>
					<input type="password" class="form-control" id="input-resetPasswordConfirm" placeholder="Password">
				</div>
			</div>
		</div>
		<div class="row mt-4 mb-4">
			<div class="col-md-4 offset-md-3">
				<div class="form-group">
					<button class="btn btn-primary btn-block" id="btn-resetPasswordSubmit">送出</button>
				</div>
			</div>
		</div>
	</div>
</div>

<script src="https://cdn.jsdelivr.net/gh/emn178/js-sha256/src/sha256.js"></script>
